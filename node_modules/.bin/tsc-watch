#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/suleyman/Desktop/snap-module-starter/node_modules/.pnpm/tsc-watch@6.0.4_typescript@5.3.3/node_modules/tsc-watch/dist/lib/node_modules:/home/suleyman/Desktop/snap-module-starter/node_modules/.pnpm/tsc-watch@6.0.4_typescript@5.3.3/node_modules/tsc-watch/dist/node_modules:/home/suleyman/Desktop/snap-module-starter/node_modules/.pnpm/tsc-watch@6.0.4_typescript@5.3.3/node_modules/tsc-watch/node_modules:/home/suleyman/Desktop/snap-module-starter/node_modules/.pnpm/tsc-watch@6.0.4_typescript@5.3.3/node_modules:/home/suleyman/Desktop/snap-module-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/suleyman/Desktop/snap-module-starter/node_modules/.pnpm/tsc-watch@6.0.4_typescript@5.3.3/node_modules/tsc-watch/dist/lib/node_modules:/home/suleyman/Desktop/snap-module-starter/node_modules/.pnpm/tsc-watch@6.0.4_typescript@5.3.3/node_modules/tsc-watch/dist/node_modules:/home/suleyman/Desktop/snap-module-starter/node_modules/.pnpm/tsc-watch@6.0.4_typescript@5.3.3/node_modules/tsc-watch/node_modules:/home/suleyman/Desktop/snap-module-starter/node_modules/.pnpm/tsc-watch@6.0.4_typescript@5.3.3/node_modules:/home/suleyman/Desktop/snap-module-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsc-watch/dist/lib/tsc-watch.js" "$@"
else
  exec node  "$basedir/../tsc-watch/dist/lib/tsc-watch.js" "$@"
fi
